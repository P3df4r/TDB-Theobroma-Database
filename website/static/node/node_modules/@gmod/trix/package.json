{
  "version": "2.0.4",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 --ext .js,.ts src ",
    "prebuild": "rimraf dist esm",
    "build:esm": "tsc --target es2018 --outDir esm --module es2020",
    "build:es5": "tsc --target es5 --outDir dist --module commonjs",
    "build": "npm run build:esm && npm run build:es5",
    "preversion": "npm run lint && npm test && npm run build",
    "postversion": "git push --follow-tags",
    "test": "jest"
  },
  "name": "@gmod/trix",
  "author": "Matt Morgan",
  "repository": "GMOD/trix-js",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.13",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "generic-filehandle": "^2.1.0",
    "jest": "^27.0.6",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
